<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_pressed="true"><shape>
			<gradient android:angle="270" android:endColor="#ffffff" android:startColor="#ededed" />
		</shape></item>
	<item android:state_selected="true"><shape>
			<gradient android:angle="270" android:endColor="#dfdfdf" android:startColor="#f0f0f0" />
		</shape></item>
	<item android:state_hovered="true"><shape>
			<gradient android:angle="270" android:endColor="#dfdfdf" android:startColor="#f0f0f0" />
		</shape></item>
	<item android:state_focused="true"><shape>
			<gradient android:angle="270" android:endColor="#dfdfdf" android:startColor="#f0f0f0" />
		</shape></item>
	<item android:state_enabled="false"><shape>
			<gradient android:angle="270" android:endColor="#c3c3c3" android:startColor="#d2d2d2" />
		</shape></item>
	<item><shape>
			<gradient android:angle="270" android:endColor="#ededed" android:startColor="#ffffff" />
		</shape></item>

</selector>